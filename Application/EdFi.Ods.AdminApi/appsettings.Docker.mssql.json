{
    "AppSettings": {
        "DefaultPageSizeOffset": 0,
        "DefaultPageSizeLimit": 25,
        "DatabaseEngine": "Mssql",
        "PathBase": "$ADMIN_API_VIRTUAL_NAME",
        "MultiTenancy": false
    },
    "Authentication": {
        "IssuerUrl": "$ISSUER_URL",
        "SigningKey": "$SIGNING_KEY",
        "AllowRegistration": true
    },
    "SwaggerSettings": {
        "EnableSwagger": true,
        "DefaultTenant": ""
    },
    "EnableDockerEnvironment": true,
    "ConnectionStrings": {
        "EdFi_Admin": "Data Source=.\\;Initial Catalog=EdFi_Admin;User Id=${MSSQL_USER};Password=${MSSQL_PASSWORD};Encrypt=false;TrustServerCertificate=true",
        "EdFi_Security": "Data Source=.\\;Initial Catalog=EdFi_Security;User Id=${MSSQL_USER};Password=${MSSQL_PASSWORD};Encrypt=false;TrustServerCertificate=true"
    },
    "Log4NetCore": {
        "Log4NetConfigFileName": "./log4net.config"
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "AllowedHosts": "*"
}
